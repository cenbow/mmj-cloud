<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mmj.notice.mapper.NoticeTemplateMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.mmj.notice.model.NoticeTemplate">
        <id column="NOTICE_ID" property="noticeId" />
        <result column="NOTICE_NAME" property="noticeName" />
        <result column="NOTICE_TYPE" property="noticeType" />
        <result column="DICT_CODE" property="dictCode" />
        <result column="DICT_VALUE" property="dictValue" />
        <result column="NOTICE_NODE" property="noticeNode" />
        <result column="SEND_TYPE" property="sendType" />
        <result column="SEND_AFTER" property="sendAfter" />
        <result column="SEND_TIME" property="sendTime" />
        <result column="MSG_TEMPLATE" property="msgTemplate" />
        <result column="NOTICE_STATUS" property="noticeStatus" />
        <result column="CREATER_ID" property="createrId" />
        <result column="CREATER_TIME" property="createrTime" />
        <result column="MODIFY_ID" property="modifyId" />
        <result column="MODIFY_TIME" property="modifyTime" />
    </resultMap>

</mapper>
